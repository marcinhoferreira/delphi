object ModelEntidadePedido: TModelEntidadePedido
  OldCreateOrder = False
  Height = 150
  Width = 215
  object qryPedidos: TFDQuery
    Connection = ModelConexao.conConexaoServidor
    SQL.Strings = (
      'SELECT numero, data_emissao, codigo_cliente, valor_total'
      'FROM pedidos'
      '&clausula_where'
      'ORDER BY numero')
    Left = 40
    Top = 8
    MacroData = <
      item
        Value = Null
        Name = 'CLAUSULA_WHERE'
      end>
  end
  object qryPedidos_Produtos: TFDQuery
    Connection = ModelConexao.conConexaoServidor
    SQL.Strings = (
      
        'SELECT id, nmero_pedido, codigo_produto, quantidade, valor_unita' +
        'rio'
      'FROM pedidos_produtos'
      'WHERE numero_pedido = :numero'
      'ORDER BY id')
    Left = 132
    Top = 8
    ParamData = <
      item
        Name = 'NUMERO'
        DataType = ftInteger
        ParamType = ptInput
        Value = Null
      end>
  end
end
